    <sql>&#x2f;&#x2a;select distinct&#xd;&#xa;cfs.ACCOUNT_FEES_DETAIL_ID id&#xd;&#xa;,cu.CUSTOMER_ID client_id&#xd;&#xa;,f.FEE_ID charge_id&#xd;&#xa;,0 as is_penalty&#xd;&#xa;,2 as charge_time_enum&#xd;&#xa;,cs.ACTION_DATE as charge_due_date&#xd;&#xa;,1 as charge_calculation_enum&#xd;&#xa;,cfs.AMOUNT amount&#xd;&#xa;,cfs.AMOUNT_PAID amount_paid_derived&#xd;&#xa;,null as amount_waived_derived&#xd;&#xa;,null as amount_writtenoff_derived&#xd;&#xa;,ifnull&#x28;cfs.AMOUNT,0&#x29; - ifnull&#x28;cfs.AMOUNT_PAID,0&#x29; amount_outstanding_derived&#xd;&#xa;,case when ifnull&#x28;cfs.AMOUNT,0&#x29; - ifnull&#x28;cfs.AMOUNT_PAID,0&#x29; &#x3d; 0 then 1 else 0 end as is_paid_derived&#xd;&#xa;,0 as waived&#xd;&#xa;,1 as is_active&#xd;&#xa;,null as inactivated_on_date&#xd;&#xa;,cfs.ACCOUNT_FEES_DETAIL_ID&#xd;&#xa;,f.FEE_ID&#xd;&#xa;,f.FEE_NAME&#xd;&#xa;,rd.RECUR_AFTER&#xd;&#xa;,rt.RECURRENCE_NAME&#xd;&#xa;,atx.RELATED_TRXN_ID&#xd;&#xa;from customer cu&#xd;&#xa;inner join account ac on ac.CUSTOMER_ID&#x3d;cu.CUSTOMER_ID and ac.ACCOUNT_TYPE_ID&#x3d;3&#xd;&#xa;inner join customer_schedule cs on cs.CUSTOMER_ID&#x3d;cu.CUSTOMER_ID&#xd;&#xa;inner join customer_fee_schedule cfs on cfs.ID&#x3d;cs.ID&#xd;&#xa;inner join account_fees af on af.ACCOUNT_FEE_ID&#x3d; cfs.ACCOUNT_FEE_ID&#xd;&#xa;inner join fees f on f.FEE_ID&#x3d;af.FEE_ID&#xd;&#xa;inner join fee_frequency ff on ff.FEE_ID&#x3d;f.FEE_ID&#xd;&#xa;inner join fee_frequency_type fft on fft.FEE_FREQUENCY_ID&#x3d;ff.FEE_FREQUENCYTYPE_ID&#xd;&#xa;inner join lookup_value lv on lv.LOOKUP_ID&#x3d;fft.LOOKUP_ID&#xd;&#xa;inner join meeting m on m.MEETING_ID&#x3d;ff.FREQUENCY_MEETING_ID&#xd;&#xa;inner join recurrence_detail rd on rd.MEETING_ID&#x3d;m.MEETING_ID&#xd;&#xa;inner join recurrence_type rt on rt.RECURRENCE_ID&#x3d;rd.RECURRENCE_ID&#xd;&#xa;left join account_trxn atx on atx.ACCOUNT_ID&#x3d;ac.ACCOUNT_ID and atx.INSTALLMENT_ID&#x3d;cs.INSTALLMENT_ID&#xd;&#xa;left join fee_trxn_detail ftd on ftd.ACCOUNT_TRXN_ID&#x3d;atx.ACCOUNT_TRXN_ID and ftd.ACCOUNT_FEE_ID&#x3d;af.ACCOUNT_FEE_ID&#xd;&#xa;&#x23;where &#xd;&#xa;&#x23;atx.RELATED_TRXN_ID is not null&#xd;&#xa;&#x23;ac.ACCOUNT_ID&#x3d;85176&#xd;&#xa;&#x23;cfs.ACCOUNT_FEES_DETAIL_ID&#x3d;904221&#xd;&#xa;&#x23;and f.CATEGORY_ID&#x3d;2&#xd;&#xa;&#x23;and atx.ACCOUNT_ACTION_ID&#x3d;12&#x2a;&#x2f;&#xd;&#xa;&#xd;&#xa;select distinct&#xd;&#xa;&#xd;&#xa;cfs.ACCOUNT_FEES_DETAIL_ID id&#xd;&#xa;,cs.CUSTOMER_ID client_id&#xd;&#xa;,cfs.FEE_ID charge_id&#xd;&#xa;,0 as is_penalty&#xd;&#xa;,2 as charge_time_enum&#xd;&#xa;,cs.ACTION_DATE as charge_due_date&#xd;&#xa;,1 as charge_calculation_enum&#xd;&#xa;,cfs.AMOUNT amount&#xd;&#xa;,cfs.AMOUNT_PAID amount_paid_derived&#xd;&#xa;,null as amount_waived_derived&#xd;&#xa;,null as amount_writtenoff_derived&#xd;&#xa;,ifnull&#x28;cfs.AMOUNT,0&#x29; - ifnull&#x28;cfs.AMOUNT_PAID,0&#x29; amount_outstanding_derived&#xd;&#xa;,case when ifnull&#x28;cfs.AMOUNT,0&#x29; - ifnull&#x28;cfs.AMOUNT_PAID,0&#x29; &#x3d; 0 then 1 else 0 end as is_paid_derived&#xd;&#xa;,0 as waived&#xd;&#xa;,1 as is_active&#xd;&#xa;,null as inactivated_on_date&#xd;&#xa;&#x2f;&#x2a;,cfs.ACCOUNT_FEES_DETAIL_ID&#xd;&#xa;,f.FEE_ID&#xd;&#xa;,f.FEE_NAME&#xd;&#xa;,rd.RECUR_AFTER&#xd;&#xa;,rt.RECURRENCE_NAME&#xd;&#xa;,atx.RELATED_TRXN_ID&#x2a;&#x2f;&#xd;&#xa;from customer_fee_schedule cfs&#xd;&#xa;inner join customer_schedule cs on cs.ID&#x3d;cfs.ID&#xd;&#xa;inner join customer cu on cu.CUSTOMER_ID&#x3d;cs.CUSTOMER_ID and cu.CUSTOMER_LEVEL_ID&#xd;&#xa;inner join account ac on ac.CUSTOMER_ID&#x3d;cu.CUSTOMER_ID and ac.ACCOUNT_TYPE_ID&#x3d;3&#xd;&#xa;&#xd;&#xa;&#x23;where cs.CUSTOMER_ID&#x3d;17177&#xd;&#xa;</sql>
