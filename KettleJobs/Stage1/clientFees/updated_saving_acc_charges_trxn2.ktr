    <sql>select &#xd;&#xa;concat&#x28;&#x27;CDA&#x27;,cu.CUSTOMER_ID&#x29; As account_no,&#xd;&#xa;NULL   AS    external_id,&#xd;&#xa;cu.CUSTOMER_ID as client_id,&#xd;&#xa;NULL as group_id,&#xd;&#xa;NULL as field_officer_id,&#xd;&#xa;case cu.status_id&#xd;&#xa;    when 6 then 600&#xd;&#xa;	 when 3  then 300&#xd;&#xa;	 when 4 then 300  end AS    status_enum,&#xd;&#xa;1   AS    account_type_enum,&#xd;&#xa;100   AS    deposit_type_enum,&#xd;&#xa;cu.CREATED_DATE AS submittedon_date,&#xd;&#xa;cu.CREATED_BY As submittedon_userid,&#xd;&#xa;cu.CUSTOMER_ACTIVATION_DATE As approvedon_date,&#xd;&#xa;cu.created_by As approvedon_userid,&#xd;&#xa;NULL   AS    rejectedon_date,&#xd;&#xa; NULL   AS    rejectedon_userid,&#xd;&#xa; NULL   AS    withdrawnon_date,&#xd;&#xa; NULL   AS    withdrawnon_userid,&#xd;&#xa;cu.customer_activation_date  AS    activatedon_date,&#xd;&#xa;cu.created_by As activatedon_userid,&#xd;&#xa;case cu.status_id &#xd;&#xa;    when 5 or 6 then cu.updated_date&#xd;&#xa;	 else null end As closedon_date,&#xd;&#xa;case cu.status_id &#xd;&#xa;    when  6 then cu.updated_by&#xd;&#xa;	 else null end  AS    closedon_userid,&#xd;&#xa; &#x24;&#x7b;CUR&#x7d; As currency_code,&#xd;&#xa; 2   AS    currency_digits,&#xd;&#xa;0   AS    currency_multiplesof,&#xd;&#xa;0   AS    nominal_annual_interest_rate,&#xd;&#xa;7   AS    interest_compounding_period_enum,&#xd;&#xa;7   AS    interest_posting_period_enum,&#xd;&#xa;1   AS    interest_calculation_type_enum,&#xd;&#xa;365   AS    interest_calculation_days_in_year_type_enum,&#xd;&#xa; NULL   AS    min_required_opening_balance,&#xd;&#xa; NULL   AS    lockin_period_frequency,&#xd;&#xa; NULL   AS    lockin_period_frequency_enum,&#xd;&#xa;0   AS    withdrawal_fee_for_transfer,&#xd;&#xa;0   AS    allow_overdraft,&#xd;&#xa; NULL   AS    overdraft_limit,&#xd;&#xa; NULL   AS    lockedin_until_date_derived,&#xd;&#xa; NULL   AS    total_deposits_derived,&#xd;&#xa; NULL   AS    total_withdrawals_derived,&#xd;&#xa; NULL   AS    total_withdrawal_fees_derived,&#xd;&#xa; NULL   AS    total_fees_charge_derived,&#xd;&#xa; NULL   AS    total_penalty_charge_derived,&#xd;&#xa; NULL   AS    total_annual_fees_derived,&#xd;&#xa; NULL   AS    total_interest_earned_derived,&#xd;&#xa; NULL   AS    total_interest_posted_derived,&#xd;&#xa; 0   AS    account_balance_derived&#xd;&#xa;from customer cu&#xd;&#xa;where cu.CUSTOMER_LEVEL_ID&#x3d;1 and cu.STATUS_ID not in &#x28;1,2,5&#x29;&#xd;&#xa;and cu.branch_id in &#x28;8,2&#x29;</sql>
